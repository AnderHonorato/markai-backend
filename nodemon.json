{
  "watch": ["src"],
  "ext": "js,json",
  "ignore": [
    "src/auth_sessions/**",
    "auth_sessions/**",
    "**/auth_sessions/**",
    "**/node_modules/**",
    "**/.wwebjs_auth/**",
    "**/.wwebjs_cache/**"
  ],
  "delay": "5000",
  "env": {
    "NODE_ENV": "development"
  },
  "verbose": false
}